<!DOCTYPE html>
<html>
<head>
    <title>Yetenek Kartım</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Skills Profile Widget template Responsive, Login form web template,Flat Pricing tables,Flat Drop downs  Sign up Web Templates, Flat Web Templates, Login sign up Responsive web template, SmartPhone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!--Custom Theme files-->
    <link href="/AdminLTE-3.0.4/web/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <!--//Custom Theme files -->
    <!--web font-->
    <link href='//fonts.googleapis.com/css?family=Questrial' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic' rel='stylesheet' type='text/css'>
    <!--//web font-->
    <!--circle-chart-->
    <script src="/AdminLTE-3.0.4/web/js/jquery-1.11.1.min.js"></script>
    <!--//circle-chart-->
    <!--ResponsiveTabs-->
    <script src="/AdminLTE-3.0.4/web/js/easyResponsiveTabs.js" type="text/javascript"></script>
    <script type="text/javascript">
	$(document).ready(function () {
		$('#horizontalTab').easyResponsiveTabs({
			type: 'default', //Types: default, vertical, accordion
			width: 'auto', //auto or any width like 600px
			fit: true   // 100% fit in a container
		});
	});
    </script>
    <!--//ResponsiveTabs-->
</head>
<body>
    <!-- main -->
    <div class="main">
        <h1>Yetenek Kartım</h1>
        <div class="main-info w3l">
            <div class="main-row">
                <!-- main-row-one -->
                <div class="profile-grid logo wthree">
                    <img src="/AdminLTE-3.0.4/web/images/melih2.jpeg" alt="" width="180" height="200">
                    <h2>@Html.DisplayFor(x => x.FirstOrDefault().NameSurname)</h2>
                    <p>@Html.DisplayFor(x => x.FirstOrDefault().Information)</p>

                </div>
                <h3 class="title">Yetenklerim</h3>
                <div class="col-md-6 skills-right bar_group">
                    <div class='bar_group__bar thin' label='@Html.DisplayFor(x => x.FirstOrDefault().Ability1) &nbsp;&nbsp; @Html.DisplayFor(x => x.FirstOrDefault().Ability1Rate)%' value='100'></div>
                    <div class='bar_group__bar thin' label='@Html.DisplayFor(x => x.FirstOrDefault().Ability2) &nbsp;&nbsp;  @Html.DisplayFor(x => x.FirstOrDefault().Ability2Rate)%' value='95'></div>
                    <div class='bar_group__bar thin' label=' @Html.DisplayFor(x => x.FirstOrDefault().Ability3) &nbsp;&nbsp;  @Html.DisplayFor(x => x.FirstOrDefault().Ability3Rate)%' value='90'></div>
                    <div class='bar_group__bar thin' label=' @Html.DisplayFor(x => x.FirstOrDefault().Ability4) &nbsp;&nbsp;  @Html.DisplayFor(x => x.FirstOrDefault().Ability4Rate)%' value='95'></div>
                </div>
                <!--bar-js-->
                <script src="/AdminLTE-3.0.4/web/js/bars.js"></script>
                <div class="social-icons agileits">
                    <ul>
                        <li><a href="#" class="fb"></a></li>
                        <li><a href="#" class="fa"></a></li>
                        <li><a href="@Html.DisplayFor(x => x.FirstOrDefault().SocialMediaLink1)" class="be"></a></li>
                        <li><a href="#" class="gg"></a></li>
                    </ul>
                </div>
                <div>
                    <a class="text" href="/AdminHeading/Index" style="color:white">Giriş Yapma Sayfasına Git</a>
                </div>
                <div class="clear"> </div>
            </div>
        </div>       
    </div>    
</body>
</html>
